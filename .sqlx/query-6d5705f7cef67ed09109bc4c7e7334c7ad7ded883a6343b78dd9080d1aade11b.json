  "query": "\n            SELECT t.id, t.slug, t.name, t.description, t.pinned, t.created_at, t.updated_at\n            FROM tags t\n            INNER JOIN post_tags pt ON pt.tag_id = t.id\n            WHERE pt.post_id = $1\n            ORDER BY t.name ASC\n            ",
